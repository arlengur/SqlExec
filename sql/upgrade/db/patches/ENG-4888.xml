    <changeSet id="20160425-01" author="kzakharov">
        <comment>
            Column to store the authorization id generated by the insurance
        </comment>
        <sql>
            alter table `prodlims`.`casefile_insurance_authorize` add column `authorizationId` VARCHAR(255);
