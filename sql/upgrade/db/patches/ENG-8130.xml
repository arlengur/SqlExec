<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="ENG-8130" author="mdubrovin">
        <comment>Add research flag to contact info (GDPR)</comment>
        <sql>
            ALTER TABLE `contactinfo` ADD COLUMN `researchOptIn` bit(1) DEFAULT NULL AFTER `nyOptIn`;
        </sql>
        <rollback>
            ALTER TABLE `contactinfo` DROP COLUMN `researchOptIn`;
        </rollback>
    </changeSet>

</databaseChangeLog>
