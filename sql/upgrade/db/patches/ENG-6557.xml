<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="ENG-6557-01" author="mdaly">
        <comment>Add String for the gene on Alpha-Thalassemia results</comment>
        <sql>
            ALTER TABLE `prodlims`.`HorizonCloudResult`
                ADD COLUMN `athalGene` VARCHAR(20) AFTER `exonRange`;
        </sql>

        <rollback>
            ALTER TABLE `prodlims`.`HorizonCloudResult`
                DROP COLUMN `athalGene`;
        </rollback>
    </changeSet>
</databaseChangeLog>