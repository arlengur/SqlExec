<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="ENG-6320" author="isavinov">
        <comment>
            Columns to store if patient is enrolled in Automatic Redraw Program
        </comment>
        <sql>
            alter table `prodlims`.`paternity_fields` add column `auto_redraw_enroll` VARCHAR(255) DEFAULT NULL;
        </sql>
        <rollback>
            alter table `prodlims`.`paternity_fields` drop column `auto_redraw_enroll`;
        </rollback>
    </changeSet>

</databaseChangeLog>

