    <changeSet id="ENG-6957-01" author="dchettupuzha">
        <comment>Add new QC into QC_TYPE table</comment>
        <sql>
            insert into `prodlims`.`qc_type` (`category`, `description`, `name`, `ismultisample`) values
            ('SAMPLE', '', 'NA12878_IDENTITY_CHECK', 0);
        </sql>
        <rollback>
            SET SQL_SAFE_UPDATES=0;
            DELETE FROM `prodlims`.`qc_type` WHERE `name`='NA12878_IDENTITY_CHECK';
            SET SQL_SAFE_UPDATES=1;
        </rollback>
    </changeSet>

